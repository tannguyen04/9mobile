{
  "name": "demo-test",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "susy": "~2.2.6",
    "slick-carousel": "^1.8.1",
    "reset-css": "latest",
    "enquire": "latest",
    "media-match": "latest",
    "ResponsiveMultiLevelMenu": "https://github.com/codrops/ResponsiveMultiLevelMenu.git"
  },
  "install": {
    "path": "../bower_components",
    "sources": {
      "susy": [
        "bower_components/susy/sass/**"
      ],
      "slick-carousel": {
        "mapping": [
          {
            "bower_components/slick-carousel/slick/slick.scss": "_slick.scss"
          },
          {
            "bower_components/slick-carousel/slick/slick.min.js": "slick.min.js"
          }
        ]
      },
      "reset-css": [
        "bower_components/reset-css/_reset.scss"
      ],
      "ResponsiveMultiLevelMenu": [
        "bower_components/ResponsiveMultiLevelMenu/js/**"
      ]
    }
  },
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "devDependencies": {
    "jquery-bar-rating": "^1.2.2"
  }
}
